<> a d2r:Server;
      # Graph metadata
      meta:datasetTitle "Linked Structured Product Labels" ;
      meta:datasetDescription "A Linked Open Data representation of Structured Product Labels (SPLs) for over 35,000 prescription and over-the-counter drug products. Each SPL is represented as an RDF graph that contains the raw text for each product label section, the NDF-RT druc class of each active ingredient in a drug product, and links to RxNorm, www.drugbank.ca/, and ChEBI." ;
      meta:datasetSource "The National Library of Medicine's LinkedSPLs_Vocabulary: http://dailymed.nlm.nih.gov/dailymed/" ;
      meta:operatorName "Richard D. Boyce, PhD" ;
      meta:operatorHomepage  <http://www.linkedin.com/in/boycer>;

      rdfs:label "Linked Structured Product Labels";
      # other dataset configuration options (e.g. d2r:baseURI, d2r:port)
      .


