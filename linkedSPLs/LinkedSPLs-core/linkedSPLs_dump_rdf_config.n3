map:DrugsRdfTypeBridge a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:structuredProductLabelMetadata;
    d2rq:property rdf:type;
    d2rq:constantValue <http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#Label>;
    .

map:HomepageURL a d2rq:PropertyBridge;
      d2rq:belongsToClassMap map:structuredProductLabelMetadata;
      d2rq:property foaf:homepage;
      d2rq:uriSqlExpression "CONCAT('http://dailymed.nlm.nih.gov/dailymed/lookup.cfm?setid=', structuredProductLabelMetadata.setId)";
      .

# Table  FDAPharmgxTable
map:FDAPharmgxTable a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	#d2rq:uriPattern "FDAPharmgxTable/@@FDAPharmgxTable.id@@";
        d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/linkedspls.FDAPharmgxTable:', CAST(FDAPharmgxTable.id AS CHAR))";

	d2rq:class linkedspls_vocabulary:pharmgxData;
	d2rq:classDefinitionLabel " Data from the FDA's pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm>";
        .

map:SPLToPharmgxData a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:pharmgxData;
	d2rq:join "structuredProductLabelMetadata.setId => FDAPharmgxTable.setId";
	#d2rq:uriSqlExpression "CONCAT('http://dbmi-icode-01.dbmi.pitt.edu/linkedspls/resource/FDAPharmgxTable/', CAST(FDAPharmgxTable.id AS CHAR))";
	d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/linkedspls.FDAPharmgxTable:', CAST(FDAPharmgxTable.id AS CHAR))";
	d2rq:propertyDefinitionLabel "pharmgxData"@en;
	d2rq:propertyDefinitionComment "Data provided by the FDA in SPLs for making therapeutic decisions using pharmacogenetic and pharmacogenomic information"@en;
	.

map:pharmgxBiomarker a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:pharmgxBiomarker;
	d2rq:column "FDAPharmgxTable.biomarker";
	d2rq:propertyDefinitionLabel "pharmgxBiomarker"@en;
	d2rq:propertyDefinitionComment "The string that the FDA provides for the biomarker in its pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm>"@en;	
	.

map:pharmgxGeneSymbol a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:HGNCGeneSymbol;
	d2rq:join "FDAPharmgxTable.biomarker => FDAPharmgxTableToOntologyMap.HGNCGeneSymbol";
	d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/hugo:', FDAPharmgxTableToOntologyMap.HGNCGeneSymbol)";
	d2rq:propertyDefinitionLabel "HGNCGeneSymbol"@en;
	d2rq:propertyDefinitionComment "The official gene symbol from the HUGO Gene Nomenclature Committee (HGNC) <www.genenames.org/> for a string that the FDA provides for the biomarker in its pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm>"@en;	
	.

map:pharmgxAlleleVariant a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:pharmgxAlleleVariant;
	d2rq:join " FDAPharmgxTable.biomarker => FDAPharmgxTableToOntologyMap.HGNCGeneSymbol";
	d2rq:column "FDAPharmgxTableToOntologyMap.AlleleVariant";
	d2rq:propertyDefinitionLabel "pharmgxAlleleVariant"@en;
	d2rq:propertyDefinitionComment "The allele variant indicated by the string that FDA provides for the biomarker in its pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm>"@en;	
	.

map:pharmgxPharmgkb a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property foaf:homepage;
	d2rq:join " FDAPharmgxTable.biomarker => FDAPharmgxTableToOntologyMap.HGNCGeneSymbol";
	d2rq:uriSqlExpression "CONCAT('http://pharmgkb.org/gene/', FDAPharmgxTableToOntologyMap.Pharmgkb)";
	.

map:pharmgxBio2rdf a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:pharmgxXref;
	d2rq:join " FDAPharmgxTable.biomarker => FDAPharmgxTableToOntologyMap.HGNCGeneSymbol";
	d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/pharmgkb:', FDAPharmgxTableToOntologyMap.Pharmgkb)";
	d2rq:propertyDefinitionLabel "pharmgxXref"@en;
	d2rq:propertyDefinitionComment "A URI that links a biomarker present in the FDA pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm> to an external Semantic Web source"@en;	
	.

map:pharmgxPRoEntity a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property owl:sameAs;
	d2rq:join " FDAPharmgxTable.biomarker => FDAPharmgxTableToOntologyMap.HGNCGeneSymbol";
	d2rq:uriColumn "FDAPharmgxTableToOntologyMap.URI";
	.

map:pharmgxComment a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property rdfs:comment;
	d2rq:join " FDAPharmgxTable.biomarker => FDAPharmgxTableToOntologyMap.HGNCGeneSymbol";
	d2rq:column "FDAPharmgxTableToOntologyMap.CuratorComments";
	.

map:pharmgxActiveMoiety a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:pharmgxDrug;
	d2rq:column "FDAPharmgxTable.activeMoiety";
	d2rq:propertyDefinitionLabel "pharmgxDrug"@en;
	d2rq:propertyDefinitionComment "The drug string that the FDA provides in its pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm>"@en;	
	.

map:pharmgxTherAppl a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:therapeuticApplication;
	d2rq:uriColumn "FDAPharmgxTable.therapeuticApplication";
	d2rq:translateWith map:pharmgxTherAppTranslate;
	d2rq:propertyDefinitionLabel "therapeuticApplication"@en;
	d2rq:propertyDefinitionComment "The therapeutic application that the FDA provides in its pharmacogenomic biomarker table  <http://www.fda.gov/Drugs/ScienceResearch/ResearchAreas/Pharmacogenetics/ucm083378.htm>"@en;	
	.

map:pharmgxRxCuiActiveMoiety a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:activeMoietyRxCUI;
	d2rq:join "FDAPharmgxTable.activeMoiety <= FDAPreferredSubstanceToRxNORM.PreferredSubstance";
	d2rq:uriColumn "FDAPreferredSubstanceToRxNORM.RxNORM";
	.


map:pharmgxSetId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:setId;
	d2rq:column "FDAPharmgxTable.setId"
	.

map:pharmgxSection a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FDAPharmgxTable;
	d2rq:property linkedspls_vocabulary:pharmgxSPLSection;
	d2rq:uriColumn "FDAPharmgxTable.SPLSection";
	d2rq:translateWith map:pharmgxSectionTranslate;
	.

