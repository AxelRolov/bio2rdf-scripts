# Table structuredProductLabelMetadata 
map:structuredProductLabelMetadata a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	#d2rq:uriPattern "structuredProductLabelMetadata/@@structuredProductLabelMetadata.setId@@";
        d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/linkedspls:', structuredProductLabelMetadata.setId)";
	d2rq:class linkedspls_vocabulary:structuredProductLabelMetadata;
        .

map:structuredProductLabelMetadata__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property rdfs:label;
	d2rq:pattern "@@structuredProductLabelMetadata.fullName@@ (@@structuredProductLabelMetadata.representedOrganization@@) setid:@@structuredProductLabelMetadata.setId@@";
	.

map:setId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:setId;
	d2rq:column "structuredProductLabelMetadata.setId";
	d2rq:propertyDefinitionLabel "setId"@en;
	d2rq:propertyDefinitionComment "The setId is the unique identifier for a particular Structured Product Label assigned by the FDA and use to track the labels in DailyMed"@en;
	.


map:versionNumber a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:versionNumber;
	d2rq:column "structuredProductLabelMetadata.versionNumber";
	d2rq:propertyDefinitionLabel "versionNumber"@en;
	d2rq:propertyDefinitionComment "The version number represents the version of a particular SPL"@en;	.

map:effectiveTime a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:effectiveTime;
	d2rq:column "structuredProductLabelMetadata.effectiveTime";
	d2rq:propertyDefinitionLabel "effectiveTime"@en;
	d2rq:propertyDefinitionComment "The effective time is the date of release for a particular version of a SPL "@en;
	.

map:fullName a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:fullName;
	d2rq:column "structuredProductLabelMetadata.fullName";
	d2rq:propertyDefinitionLabel "fullName"@en;
	d2rq:propertyDefinitionComment "The title given to a SPL"@en;
	.


map:representedOrganization a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:representedOrganization;
	d2rq:column "structuredProductLabelMetadata.representedOrganization";
	d2rq:propertyDefinitionLabel "representedOrganization"@en;
	d2rq:propertyDefinitionComment "The company that manufactured the product referred to in an SPL"@en;
	.


# map active moiety UNII
map:activeMoietyUNII a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:activeMoiety;
	d2rq:join "structuredProductLabelMetadata.id = spl_has_active_moiety.spl";
	d2rq:join "spl_has_active_moiety.active_moiety = active_moiety.id";

	d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/linkedspls:', active_moiety.UNII)";
	d2rq:propertyDefinitionLabel "activeMoietyUNII"@en;
	d2rq:propertyDefinitionComment "The FDA Unique Ingredient Identifier an active moiety contained in the drug product (see http://www.fda.gov/ForIndustry/DataStandards/StructuredProductLabeling/ucm162523.htm)"@en;
	.


map:drugRxNormSubject a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property dc:subject;
	d2rq:join "structuredProductLabelMetadata.setId = SPLSetIDToRxNORM.setId";
	d2rq:uriSqlExpression "CONCAT('http://purl.bioontology.org/ontology/RXNORM/', SPLSetIDToRxNORM.RxCUI)";
	.

## Sections mappings	
map:genericMedicine a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:genericMedicine;
	d2rq:column "structuredProductLabelMetadata.genericMedicine";
	d2rq:propertyDefinitionLabel "genericMedicine"@en;
	d2rq:propertyDefinitionComment "The generic name of all active ingredients in a drug product"@en;
	.

map:routeOfAdministration a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:structuredProductLabelMetadata;
	d2rq:property linkedspls_vocabulary:routeOfAdministration;

	#d2rq:uriPattern "routeOfAdministration/@@structuredProductLabelMetadata.routeOfAdministration|urlify@@";
	d2rq:uriSqlExpression "CONCAT('http://bio2rdf.org/linkedspls_vocabulary:routeOfAdministration#', structuredProductLabelMetadata.routeOfAdministration)";

	d2rq:propertyDefinitionLabel "routeOfAdministration"@en;
	d2rq:propertyDefinitionComment "The route by which a drug product is taken into the body"@en;
	.

map:supply a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:structuredProductLabelMetadata;
    d2rq:property loinc:34069-5;
    d2rq:join "structuredProductLabelMetadata.id = how_supplied.splId";
    d2rq:column "how_supplied.field";
    d2rq:propertyDefinitionLabel "supply";
    d2rq:propertyDefinitionComment "Specific information about how supplied for a drug product. The text is taken verbatim from the HOW SUPPLIED SECTION of product's SPL. Information in tables has been retained in HTML to support NLP programs. The label of this property lists the LOINC code for this section (see http://www.fda.gov/ForIndustry/DataStandards/StructuredProductLabeling/ucm162057.htm)"@en;
    .

map:boxed_warning a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:structuredProductLabelMetadata;
    d2rq:property loinc:34066-1;
    d2rq:join "structuredProductLabelMetadata.id = boxed_warning.splId";
    d2rq:column "boxed_warning.field";
    d2rq:propertyDefinitionLabel "boxedWarning";
    d2rq:propertyDefinitionComment "Specific information about boxed warning for a drug product. The text is taken verbatim from the BOXED WARNING SECTION of product's SPL. Information in tables has been retained in HTML to support NLP programs. The label of this property lists the LOINC code for this section (see http://www.fda.gov/ForIndustry/DataStandards/StructuredProductLabeling/ucm162057.htm)"@en;
    .


map:indications_and_usage a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:structuredProductLabelMetadata;
    d2rq:property loinc:34067-9;
    d2rq:join "structuredProductLabelMetadata.id = indications_and_usage.splId";
    d2rq:column "indications_and_usage.field";
    d2rq:propertyDefinitionLabel "indicationsAndUsage";
    d2rq:propertyDefinitionComment "Specific information about indications and usage for a drug product. The text is taken verbatim from the INDICATIONS & USAGE SECTION of product's SPL. Information in tables has been retained in HTML to support NLP programs. The label of this property lists the LOINC code for this section (see http://www.fda.gov/ForIndustry/DataStandards/StructuredProductLabeling/ucm162057.htm)"@en;
    .


map:dosage_and_administration a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:structuredProductLabelMetadata;
    d2rq:property loinc:34068-7;
    d2rq:join "structuredProductLabelMetadata.id = dosage_and_administration.splId";
    d2rq:column "dosage_and_administration.field";
    d2rq:propertyDefinitionLabel "dosageAndAdministration";
    d2rq:propertyDefinitionComment "Specific information about dosage and administration for a drug product. The text is taken verbatim from the DOSAGE & ADMINISTRATION SECTION of product's SPL. Information in tables has been retained in HTML to support NLP programs. The label of this property lists the LOINC code for this section (see http://www.fda.gov/ForIndustry/DataStandards/StructuredProductLabeling/ucm162057.htm)"@en;
    .


